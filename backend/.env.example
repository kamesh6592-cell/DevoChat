# Backend Environment Variables Template
# Copy this file to .env and fill in your actual values

# MongoDB Connection
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/chat_db

# Frontend URLs
PRODUCTION_URL=https://your-app-name.vercel.app
DEVELOPMENT_URL=http://localhost:3000

# Authentication Secret Key (generate with: python generate_secret.py)
AUTH_KEY=your-random-secret-key-here

# AI Provider API Keys (add at least one)
# OpenAI
OPENAI_API_KEY=sk-...

# Anthropic (Claude)
ANTHROPIC_API_KEY=sk-ant-...

# Google (Gemini)
GEMINI_API_KEY=...

# Perplexity
PERPLEXITY_API_KEY=...

# HuggingFace
HUGGINGFACE_API_KEY=...

# X.AI (Grok)
XAI_API_KEY=...

# Mistral
MISTRAL_API_KEY=...

# Fireworks
FIREWORKS_API_KEY=...

# Friendli
FRIENDLI_API_KEY=...

# Flux (Image Generation)
FLUX_API_KEY=...

# BytePlus
BYTEPLUS_API_KEY=...

# Alibaba
ALIBABA_API_KEY=...
